{
    "name": "terabaud/php-mail-api",
    "description": "A simple API for sending emails",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Lea Rosema",
            "email": "terabaud@gmail.com",
            "homepage": "http://terabaud.github.io/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "google/recaptcha": "~1.1",
        "php-mock/php-mock": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "php-mock/php-mock-phpunit": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/middleware/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
